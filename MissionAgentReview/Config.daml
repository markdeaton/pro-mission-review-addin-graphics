  <conditions>
    <!-- our custom condition -->
    <insertCondition id="AgentTrackPathsButtonEnabled_Condition" caption="Agent Track Review Button Enabled?">
      <!-- our condition is set true or false based on this underlying state -->
      <state id="trackFeatureLayerSelected_state"/>
    </insertCondition>
  </conditions>
  
  <modules>
    <insertModule id="MissionAgentReview_Module" className="Module1" autoLoad="true" caption="Module1">
      <!-- uncomment to have the control hosted on a separate tab-->
      <tabs>
        <!--<tab id="MissionAgentReview_Tab1" caption="New Tab">
          <group refID="MissionAgentReview_Group1"/>
        </tab>-->
      </tabs>
      <groups>
        <!-- comment this out if you have no controls on the Addin tab to avoid
              an empty group-->
        <group id="MissionAgentReview_Group1" caption="Mission Review" appearsOnAddInTab="true">
          <!-- host controls within groups -->
          <button refID="MissionAgentReview_Button1" size="large" />
        </group>
      </groups>
      <controls>
        <!-- add your controls here -->
        <button id="MissionAgentReview_Button1" caption="Agent Track Paths" className="AgentTrackPathButton" loadOnClick="true" 
                 
                smallImage="pack://application:,,,/ArcGIS.Desktop.Resources;component/Images/GenericButtonBlue16.png" 
                largeImage="pack://application:,,,/ArcGIS.Desktop.Resources;component/Images/GenericButtonBlue32.png">
          <tooltip heading="Agent Track Analysis">A tool to assist in analyzing agent tracks post-mission<disabledText /></tooltip>
        </button>
      </controls>
    </insertModule>
  </modules>
